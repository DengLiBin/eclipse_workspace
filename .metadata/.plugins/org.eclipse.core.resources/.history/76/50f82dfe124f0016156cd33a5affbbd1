	@Test
	public void testEnum(){
		System.out.println(Direction.E);
		Direction.E.print();
		
		switch(Direction.E){
			case Direction.E:
				
				break;
		}
	}
	//测试带抽象方法的枚举
	@Test
	public void testAbstractEnum(){
		String string = AbstractEnum.A.localValue();
		System.out.println(string);
	}
}
