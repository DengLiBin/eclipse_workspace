package myuse.myenum;

public enum Direction {
	E("100-90"),W("89-80"),N("79-70"),S("69-60");//Object,该枚举的对象
	private String value;//封装每个对象对应的分数
	
	//定义枚举的构造方法
	private Direction(String value){
		this.value = value;
	}
	
	public void print(Direction direction){
		System.out.println(direction.value);
	}
}
