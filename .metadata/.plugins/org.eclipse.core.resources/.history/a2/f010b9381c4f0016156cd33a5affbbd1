package myuse.domain;

public final class Person {
	private int age;
	private String name;
	public Person(){
		
	}
	public Person(int age){
		this.age = age;
	}
	
	public Person(int age, String name) {
		super();
		this.age = age;
		this.name = name;
	}
	public Person(String name){
		this.name = name;
	}
	public String getName() {
		return name;
	}
	public int getAge(){
		return age;
	}
	
}
